; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "LDtk",
"language": "GDScript",
"path": "res://addons/LDtk-Importer/LDtk.gd"
}, {
"base": "Node",
"class": "LDtkLayerInstance",
"language": "GDScript",
"path": "res://addons/LDtk-Importer/LDtkLayerInstance.gd"
}, {
"base": "Node2D",
"class": "LDtkLevel",
"language": "GDScript",
"path": "res://addons/LDtk-Importer/LDtkLevel.gd"
} ]
_global_script_class_icons={
"LDtk": "",
"LDtkLayerInstance": "",
"LDtkLevel": ""
}

[application]

config/name="Ldtk-tilemap-import"
config/icon="res://icon.png"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "LDtk-Importer" )

[rendering]

environment/default_environment="res://default_env.tres"
